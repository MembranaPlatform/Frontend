{
  "name": "membrana-site",
  "version": "1.7.1",
  "private": true,
  "dependencies": {
    "@amcharts/amcharts3-react": "^3.1.0",
    "@babel/core": "^7.6.3",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "amcharts3": "^3.21.14",
    "bignumber.js": "^7.2.1",
    "bootstrap": "^4.1.1",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.2.0",
    "jquery": "^3.4.1",
    "lodash.merge": "^4.3.2",
    "lodash.times": "^4.3.2",
    "malihu-custom-scrollbar-plugin": "^3.1.5",
    "memoize-one": "^5.0.5",
    "node-sass-chokidar": "^1.3.5",
    "qrcode.react": "^0.8.0",
    "qs": "^6.7.0",
    "react": "^16.5.2",
    "react-calendar": "^2.19.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-custom-scrollbars": "^4.2.1",
    "react-debounce-render": "^4.0.1",
    "react-dom": "^16.3.2",
    "react-intl": "^2.9.0",
    "react-modal": "^3.5.1",
    "react-redux": "^5.0.7",
    "react-responsive": "^4.1.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.5",
    "react-table": "^6.8.5",
    "reactstrap": "^6.0.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "regenerator-runtime": "^0.13.3",
    "socket.io-client": "^2.1.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --maxWorkers=3",
    "eject": "react-scripts eject",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/testConfig.js"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/public/**",
      "!**/src/demoData/**",
      "!**/src/assets/**",
      "!**/src/fonts/**"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 50
      }
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/.*/__mocks__",
      "<rootDir>/src/App.test.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|png|svg)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.2",
    "husky": "^1.0.0-rc.14",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "lint-staged": "^7.2.2",
    "npm-run-all": "^4.1.2",
    "redux-mock-store": "^1.5.3"
  },
  "proxy": "https://beta.membrana.io"
}
